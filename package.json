{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npx jest --coverage --forceExit --detectOpenHandles",
    "sonar": "node sonar-project.js",
    "format:check": "prettier --check **/*.{ts,tsx}",
    "format:fix": "prettier --write **/*.{ts,tsx}",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "pre-commit": "lint-staged",
    "dev:win": "yarn open-browser && next dev -p 3000",
    "open-browser": "start http://localhost:3000",
    "prepare": "husky install",
    "tsm": "yarn typed-scss-modules src"
  },
  "dependencies": {
    "next": "12.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.3.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/cookie": "0.5.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.8.0",
    "@types/react": "18.0.21",
    "@types/react-datepicker": "4.8.0",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-config-standard-react": "13.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-testing-library": "5.7.2",
    "husky": "8.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "lint-staged": "12.3.7",
    "msw": "1.1.0",
    "next-router-mock": "^0.9.2",
    "prettier": "2.7.1",
    "sass": "1.54.8",
    "sonarqube-scanner": "3.0.1",
    "typed-scss-modules": "7.0.1",
    "typescript": "4.8.4",
    "whatwg-fetch": "3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "packageManager": "yarn@1.22.19"
}
